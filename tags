eol="$tags0x $tags1x $tags19 $tags20 $tags21 $tags22 $tags23 $tags24 $tags25 $tags26 $tags27"

rmtags() {
  for list in $1; do
    for tag in $list; do git tag -d $tag  2>/dev/null; done
  done
}

# rmtags "$tags0x"
rmtags "$eol"
echo

for tag in $(git tag); do
  echo -n "Tag ${tag}: \t commit "
  git show --oneline $tag |head -4 |tail -1|cut -f1 -d' '
done
